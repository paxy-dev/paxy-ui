{
  "name": "paxy-ui",
  "version": "0.1.7",
  "main": "lib/index.js",
  "module": "dist/index.esm.js",
  "description": "",
  "authors": {
    "name": "yc",
    "email": "yang.chen@scilifelab.se"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paxy-dev/paxy-ui.git"
  },
  "scripts": {
    "prepare": "install-peers -f",
    "start": "cross-env APP_ROOT=example umi dev",
    "lint": "umi g tmp && yarn lint:js && yarn lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "build": "father-build",
    "prettier": "prettier --write \"src/**/*\" --end-of-line auto",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "doc:dev": "dumi dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "1.9.5",
    "@ant-design/pro-form": "1.34.0",
    "@ant-design/pro-layout": "6.24.0",
    "@ant-design/pro-table": "2.48.0",
    "@ant-design/pro-utils": "1.20.4",
    "faker": "^5.5.3",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "short-uuid": "4.1.0",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/faker": "^5.1.7",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/url-parse": "^1.4.3",
    "@umijs/preset-ui": "^2.1.13",
    "@umijs/test": "^3.0.10",
    "@umijs/test-utils": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.18.2",
    "cross-env": "^6.0.3",
    "dumi": "^1.1.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "express": "^4.15.3",
    "father-build": "1.19.2",
    "install-peers-cli": "^2.2.0",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "pify": "^5.0.0",
    "prettier": "^2.2.1",
    "puppeteer": "^1.20.0",
    "query-string": "^6.11.1",
    "react": "^16.x",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.9.0",
    "test-umi-plugin": "^0.1.0",
    "umi": "^3.x",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd": "^4.x",
    "react": "^16.x",
    "umi": "^3.x"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "dist",
    "lib"
  ]
}
